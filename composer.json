{
  "name": "brick/std",
  "description": "An attempt at a standard library for PHP",
  "type": "library",
  "keywords": [
    "Brick",
    "Std"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*",
    "php-coveralls/php-coveralls": "2.*",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "Brick\\Std\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Brick\\Std\\Tests\\": "tests/"
    }
  }
}
